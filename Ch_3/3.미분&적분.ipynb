   "source": [
    "from scipy.misc import derivative\n",
    "\n",
    "def f(x):\n",
    "    return 3*x**2+1\n",
    "\n",
    "def d(x):\n",
    "    return derivative(f,x)\n",
    "\n",
    "print(d(2.0))"
   ]
